        .SECTION    V, DATA, ALIGN=2
        .SECTION    P, CODE, ALIGN=2
        .SECTION    D, DATA, ALIGN=2
        .SECTION    R, DATA, ALIGN=2
        .SECTION    B, DATA, ALIGN=2
        .SECTION    C, DATA, ALIGN=2
__D_ROM .DATA.W     (STARTOF D)             ;ｾｸｼｮﾝD先頭ｱﾄﾞﾚｽ
__D_BGN .DATA.W     (STARTOF R)             ;ｾｸｼｮﾝR先頭ｱﾄﾞﾚｽ
__D_END .DATA.W     (STARTOF R)+(SIZEOF R)  ;ｾｸｼｮﾝR最終ｱﾄﾞﾚｽ
__B_BGN .DATA.W     (STARTOF B)             ;ｾｸｼｮﾝB先頭ｱﾄﾞﾚｽ
__B_END .DATA.W     (STARTOF B)+(SIZEOF B)  ;ｾｸｼｮﾝB最終ｱﾄﾞﾚｽ
        .EXPORT     __D_ROM
        .EXPORT     __D_BGN
        .EXPORT     __D_END
        .EXPORT     __B_BGN
        .EXPORT     __B_END
        
        .END
